{
  "name": "bchalier/laravel-openapi-autodoc",
  "description": "Automatic documentation of laravel in OpenApi format.",
  "keywords": [
    "laravel",
    "openapi",
    "open api",
    "doc",
    "documentation",
    "automation",
    "generator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Bastien Chalier",
      "email": "bastien@chalier.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "goldspecdigital/oooas": "^2.4",
    "phpdocumentor/reflection-docblock": "^5.1",
    "doctrine/annotations": "^1.8"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "psr-4": {
      "Bchalier\\LaravelOpenapiDoc\\": "."
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bchalier\\LaravelOpenapiDoc\\App\\Providers\\LoadingServiceProvider"
      ]
    }
  }
}
